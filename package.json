  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode analyze && npx vite-bundle-analyzer dist",
    "analyze": "npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "quality-check": "npm run lint && npm run type-check",
    "quality-fix": "npm run lint:fix && npm run type-check",
    "preview": "vite preview",
    "pre-commit": "npm run quality-check",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "node scripts/coverage-check.js",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.config.ts src/test/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:prod": "playwright test --config=playwright.config.prod.ts",
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:a11y": "playwright test --config=playwright.a11y.config.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "bundle-size:check": "node scripts/bundle-size-check.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:types": "supabase gen types typescript --local > src/integrations/supabase/types.ts"
